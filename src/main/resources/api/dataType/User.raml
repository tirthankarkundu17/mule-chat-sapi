#%RAML 1.0 DataType
type: object
properties:
  userId:
    description: Unique User ID for an user
    type: string
    required: false
  name:
    description: Name of the  user
    type: string
    required: true
  avatar:
    description: Avatar URL of the  user
    type: string
    required: false
  socketId:
    description: Web Socket ID of the  user
    type: string
    required: false
    